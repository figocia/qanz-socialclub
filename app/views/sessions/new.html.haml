%section.sign_in.container
  .row
    .col-sm-10
      = bootstrap_form_tag url: login_path do |f|
        %header
          %h1 Sign in
        %fieldset
        = f.email_field :email, label: 'Email Address', placeholder: 'name@example.com'
        = f.password_field :password
        = f.submit 'Log in' 
        = link_to 'Forgot password?'
            
